{
  "name": "pollbuilder",
  "version": "1.0.0",
  "description": "Build your own polls, share with your friends, vote in polls created by other users, and view results of any poll.",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "client": "cd client && yarn build && yarn start",
    "sass": "cd client && yarn sass",
    "server": "LOG_LEVEL=debug nodemon --inspect server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\" \"yarn sass\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rifkegribenes/pollbuilder.git"
  },
  "bugs": "https://github.com/rifkegribenes/pollbuilder/issues",
  "homepage": "https://github.com/rifkegribenes/pollbuilder#readme",
  "keywords": [
    "MongoDB",
    "Mongoose",
    "full stack",
    "Express",
    "Node",
    "javascript"
  ],
  "author": "Sarah Schneider",
  "license": "MIT",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "connect-mongo": "^2.0.1",
    "cors": "^2.8.4",
    "crypto": "^1.0.1",
    "dotenv": "^1.2.0",
    "express": "^4.16.2",
    "express-jwt": "^5.3.0",
    "express-session": "^1.15.6",
    "hoek": "^5.0.3",
    "jsonwebtoken": "^8.1.1",
    "method-override": "^2.3.10",
    "mongoose": "^5.0.1",
    "morgan": "^1.9.0",
    "negotiator": "^0.6.1",
    "nodemailer": "^4.6.0",
    "nodemailer-mailgun-transport": "^1.3.6",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-github2": "^0.1.11",
    "passport-google-oauth2": "^0.1.6",
    "passport-jwt": "^3.0.1",
    "passport-local": "^1.0.0",
    "sanitize-html": "^1.18.2",
    "slugify": "^1.2.9"
  }
}
